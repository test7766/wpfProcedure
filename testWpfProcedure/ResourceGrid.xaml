<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:testWpfProcedure"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors">
    <DataTemplate x:Key="Doc_TypeHeaderTemplate">
        <StackPanel Orientation="Vertical">
            <TextBlock Text="Тип документу" />
            <dxe:TextEdit x:Name="Doc_Type" Text="{Binding DocType, Mode=TwoWay}"/>
        </StackPanel>
    </DataTemplate>
    <DataTemplate x:Key="Doc_CoHeaderTemplate">
        <StackPanel Orientation="Vertical">
            <TextBlock Text="Компанія документу" />
            <dxe:TextEdit x:Name="Doc_Co" Text="{Binding DocCo, Mode=TwoWay}" />
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>
